{
	"appId": "com.look.out",
	"appName": "LookOut",
	"webDir": "dist",
	"bundledWebRuntime": false,
	"plugins": {
		"FirebaseAuthentication": {
			"skipNativeAuth": false,
			"providers": [
				"phone"
			]
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	}
}
